   "source": [
    "if df is not None and not pareto_df.empty:
",
    "    # --- 3D Scatter Plot ---
",
    "    fig = plt.figure(figsize=(10, 8))
",
    "    ax = fig.add_subplot(111, projection='3d')
",
    "    
",
    "    # Plot all points (dominated solutions)
",
    "    ax.scatter(df[objectives[0]], df[objectives[1]], df[objectives[2]], c='blue', marker='o', alpha=0.3, label='Dominated Solutions')
",
    "    
",
    "    # Plot Pareto optimal points
",
    "    ax.scatter(pareto_df[objectives[0]], pareto_df[objectives[1]], pareto_df[objectives[2]], c='red', marker='x', s=100, label='Pareto Optimal Solutions')
",
    "    
",
    "    ax.set_xlabel(objectives[0] + ' (J)')
",
    "    ax.set_ylabel(objectives[1] + ' (J)')
",
    "    ax.set_zlabel(objectives[2] + ' (J)')
",
    "    ax.set_title('Pareto Front for Energy Optimization (3D)')
",
    "    ax.legend()
",
    "    plt.tight_layout()
",
    "    plt.savefig(PARETO_PLOT_3D_FILE)
",
    "    print(f\"\nPareto front 3D plot saved to {PARETO_PLOT_3D_FILE}\")
",
    "    plt.show() # Display the plot in the notebook
",
    "    plt.close(fig)
",
    "    
",
    "    # --- Pair Plot ---
",
    "    # Add a column to distinguish Pareto points for plotting in pairplot
",
    "    df['ParetoStatus'] = 'Dominated'
",
    "    df.loc[pareto_mask, 'ParetoStatus'] = 'Optimal'
",
    "    
",
    "    print(\"\nGenerating pair plot...\")
",
    "    pair_plot = sns.pairplot(df, vars=objectives, hue='ParetoStatus', 
",
    "                             markers=['o', 'X'], palette={'Dominated': 'blue', 'Optimal': 'red'}, 
",
    "                             plot_kws={'alpha': 0.6, 's': 50}, diag_kind='kde')
",
    "    pair_plot.fig.suptitle('Pareto Front Pair Plot', y=1.02)
",
    "    pair_plot.savefig(PARETO_PAIRPLOT_FILE)
",
