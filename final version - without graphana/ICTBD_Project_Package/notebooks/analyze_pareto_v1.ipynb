    "## Load Data and Identify Objectives"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Define file paths (adjust if necessary)
",
    "SIM_OUTPUTS_FILE = \"/home/ubuntu/upload/simulation_outputs.csv\" # Input file from simulations
",
    "PARETO_SOLUTIONS_FILE = \"/home/ubuntu/pareto_optimal_solutions.csv\" # Output file
",
    "PARETO_PLOT_3D_FILE = \"/home/ubuntu/pareto_front_3d.png\" # Output plot
",
    "PARETO_PAIRPLOT_FILE = \"/home/ubuntu/pareto_front_pairplot.png\" # Output plot
",
    "
",
    "# Load the simulation results
",
    "try:
",
    "    df = pd.read_csv(SIM_OUTPUTS_FILE)
",
    "except FileNotFoundError:
",
    "    print(f\"Error: Simulation output file not found at {SIM_OUTPUTS_FILE}\")
",
    "    # Handle error appropriately, e.g., raise exception or exit
",
    "    df = None # Set df to None to prevent further execution if file not found
",
    "
",
    "# Define the objectives to minimize (ensure these column names match your CSV)
",
    "objectives = [\"Electricity:Facility\", \"DistrictHeating:Facility\", \"DistrictCooling:Facility\"]
",
    "
",
    "# Verify objectives exist in the DataFrame
",
    "if df is not None:
",
    "    missing_objectives = [obj for obj in objectives if obj not in df.columns]
",
    "    if missing_objectives:
",
    "        print(f\"Error: The following objective columns are missing in {SIM_OUTPUTS_FILE}: {missing_objectives}\")
",
    "        df = None # Prevent further execution
",
    "    else:
",
    "        print(\"Simulation data loaded successfully.\")
",
    "        print(f\"Objectives: {objectives}\")"
   ]
  },
  {
